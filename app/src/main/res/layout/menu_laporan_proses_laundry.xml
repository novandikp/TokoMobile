<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuLaporanProsesLaundry">

    <ImageView
        android:id="@+id/imageView43"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/edtPencarian"
        app:layout_constraintEnd_toStartOf="@+id/edtPencarian"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/edtPencarian"
        app:srcCompat="@drawable/ic_baseline_search_24" />

    <EditText
        android:id="@+id/edtPencarian"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="35dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:hint="Cari"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar29" />

    <Button
        android:id="@+id/button8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="#4CAF50"
        android:onClick="Excel"
        android:text="Excel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar29"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView397"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="Proses Laundry"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/Kembali"
        app:layout_constraintStart_toEndOf="@+id/Kembali"
        app:layout_constraintTop_toTopOf="@+id/Kembali" />

    <ImageView
        android:id="@+id/Kembali"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:rotation="90"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar29"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_arrow_downward_24" />

    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toTopOf="@+id/button8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView443"
        tools:listitem="@layout/laundryitemlaporanproseslaundry" />

    <TextView
        android:id="@+id/textView440"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="32dp"
        android:text="17/07/2022"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/textView441"
        app:layout_constraintEnd_toStartOf="@+id/textView441"
        app:layout_constraintTop_toTopOf="@+id/textView441" />

    <TextView
        android:id="@+id/textView441"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtPencarian" />

    <TextView
        android:id="@+id/textView442"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:text="17/07/2022"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/textView441"
        app:layout_constraintStart_toEndOf="@+id/textView441"
        app:layout_constraintTop_toTopOf="@+id/textView441" />

    <TextView
        android:id="@+id/textView443"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="Jumlah Pendapatan :"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="@+id/textView445"
        app:layout_constraintTop_toBottomOf="@+id/textView445" />

    <TextView
        android:id="@+id/textView444"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="2dp"
        android:text="Rp."
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/textView443"
        app:layout_constraintStart_toEndOf="@+id/textView443"
        app:layout_constraintTop_toTopOf="@+id/textView443" />

    <TextView
        android:id="@+id/textView445"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Jumlah Pesanan Diproses :"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView440" />

    <TextView
        android:id="@+id/textView446"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:text="4"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/textView445"
        app:layout_constraintStart_toEndOf="@+id/textView445"
        app:layout_constraintTop_toTopOf="@+id/textView445" />

    <TextView
        android:id="@+id/textView447"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="2dp"
        android:text="4000"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/textView444"
        app:layout_constraintStart_toEndOf="@+id/textView444"
        app:layout_constraintTop_toTopOf="@+id/textView444" />

</androidx.constraintlayout.widget.ConstraintLayout>
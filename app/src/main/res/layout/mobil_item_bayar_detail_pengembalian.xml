<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".MobilItemBayarDetailPengembalian">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/tvTahunKeluaran"
                android:layout_width="69dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:paddingStart="8dp"
                android:paddingTop="1dp"
                android:paddingBottom="2dp"
                android:text="Bis Bisan "
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tvPlat"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingStart="8dp"
                android:paddingTop="1dp"
                android:paddingBottom="2dp"
                android:text="04/08/2022"
                app:layout_constraintStart_toStartOf="@+id/tvTahunKeluaran"
                app:layout_constraintTop_toBottomOf="@+id/tvTahunKeluaran" />

            <TextView
                android:id="@+id/tvHarga"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                android:paddingStart="8dp"
                android:paddingTop="1dp"
                android:paddingBottom="2dp"
                android:text="1"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/tvPlat"
                app:layout_constraintTop_toBottomOf="@+id/tvPlat" />

            <TextView
                android:id="@+id/textView359"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="2dp"
                android:text="-"
                app:layout_constraintBottom_toBottomOf="@+id/tvPlat"
                app:layout_constraintStart_toEndOf="@+id/tvPlat"
                app:layout_constraintTop_toTopOf="@+id/tvPlat" />

            <TextView
                android:id="@+id/textView360"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="2dp"
                android:text="hari x"
                app:layout_constraintBottom_toBottomOf="@+id/tvHarga"
                app:layout_constraintStart_toEndOf="@+id/tvHarga"
                app:layout_constraintTop_toTopOf="@+id/tvHarga" />

            <ImageView
                android:id="@+id/imageView20"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginEnd="8dp"
                android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:srcCompat="@drawable/ic_baseline_arrow_forward_ios_24" />

            <TextView
                android:id="@+id/textView362"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="2dp"
                android:text="-"
                app:layout_constraintBottom_toBottomOf="@+id/tvTahunKeluaran"
                app:layout_constraintStart_toEndOf="@+id/tvTahunKeluaran"
                app:layout_constraintTop_toTopOf="@+id/tvTahunKeluaran" />

            <TextView
                android:id="@+id/textView363"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="2dp"
                android:text="123"
                app:layout_constraintBottom_toBottomOf="@+id/textView362"
                app:layout_constraintStart_toEndOf="@+id/textView362"
                app:layout_constraintTop_toTopOf="@+id/textView362" />

            <TextView
                android:id="@+id/textView364"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="2dp"
                android:text="05/08/2022"
                app:layout_constraintBottom_toBottomOf="@+id/textView359"
                app:layout_constraintStart_toEndOf="@+id/textView359"
                app:layout_constraintTop_toTopOf="@+id/textView359" />

            <TextView
                android:id="@+id/textView365"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="2dp"
                android:text="20.000"
                app:layout_constraintBottom_toBottomOf="@+id/textView360"
                app:layout_constraintStart_toEndOf="@+id/textView360"
                app:layout_constraintTop_toTopOf="@+id/textView360" />

            <TextView
                android:id="@+id/textView366"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="2dp"
                android:text="="
                app:layout_constraintBottom_toBottomOf="@+id/textView365"
                app:layout_constraintStart_toEndOf="@+id/textView365"
                app:layout_constraintTop_toTopOf="@+id/textView365" />

            <TextView
                android:id="@+id/textView367"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="2dp"
                android:text="20.000"
                app:layout_constraintBottom_toBottomOf="@+id/textView366"
                app:layout_constraintStart_toEndOf="@+id/textView366"
                app:layout_constraintTop_toTopOf="@+id/textView366" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>
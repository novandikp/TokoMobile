        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/tvtoko"
        app:layout_constraintStart_toEndOf="@+id/tvtoko"
        app:layout_constraintTop_toTopOf="@+id/tvtoko" />

    <TextView
        android:id="@+id/tvtoko"

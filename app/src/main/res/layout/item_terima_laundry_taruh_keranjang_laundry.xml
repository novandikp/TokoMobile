<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".ItemTerimaLaundryTaruhKeranjangLaundry">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView608"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:text="(Yadi) Nyoba"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView609"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="4dp"
                android:text="-"
                app:layout_constraintBottom_toBottomOf="@+id/textView608"
                app:layout_constraintStart_toEndOf="@+id/textView608"
                app:layout_constraintTop_toTopOf="@+id/textView608" />

            <TextView
                android:id="@+id/textView610"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="4dp"
                android:text="1"
                app:layout_constraintBottom_toBottomOf="@+id/textView609"
                app:layout_constraintStart_toEndOf="@+id/textView609"
                app:layout_constraintTop_toTopOf="@+id/textView609" />

            <TextView
                android:id="@+id/textView611"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="8dp"
                android:text="Rp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/textView608"
                app:layout_constraintTop_toBottomOf="@+id/textView608" />

            <TextView
                android:id="@+id/textView612"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="4dp"
                android:text="1.000"
                app:layout_constraintBottom_toBottomOf="@+id/textView611"
                app:layout_constraintStart_toEndOf="@+id/textView611"
                app:layout_constraintTop_toTopOf="@+id/textView611" />

            <ImageView
                android:id="@+id/imageView204"
                android:layout_width="30dp"
                android:layout_height="30dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/trash" />

            <TextView
                android:id="@+id/textView613"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="4dp"
                android:text="Pcs"
                app:layout_constraintBottom_toBottomOf="@+id/textView610"
                app:layout_constraintStart_toEndOf="@+id/textView610"
                app:layout_constraintTop_toTopOf="@+id/textView610" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>
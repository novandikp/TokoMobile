<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".ItemLaporanJasaLaundry">

    <androidx.cardview.widget.CardView
        android:layout_width="0dp"
        android:layout_height="80dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/edtDaftarPegawai"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="4dp"
                android:text="Anwar"
                android:textColor="#000000"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/edtDaftarPegawai2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:text=" 17/02/2022"
                app:layout_constraintStart_toStartOf="@+id/edtDaftarPegawai"
                app:layout_constraintTop_toBottomOf="@+id/edtDaftarPegawai" />

            <TextView
                android:id="@+id/edtDaftarPegawai3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:text="0895343428020"
                app:layout_constraintStart_toStartOf="@+id/edtDaftarPegawai2"
                app:layout_constraintTop_toBottomOf="@+id/edtDaftarPegawai2" />

            <ImageView
                android:id="@+id/imageView139"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginEnd="16dp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView140"
                app:layout_constraintEnd_toStartOf="@+id/imageView140"
                app:layout_constraintTop_toTopOf="@+id/imageView140"
                app:srcCompat="@drawable/telepon" />

            <ImageView
                android:id="@+id/imageView140"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginEnd="16dp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView141"
                app:layout_constraintEnd_toStartOf="@+id/imageView141"
                app:layout_constraintTop_toTopOf="@+id/imageView141"
                app:srcCompat="@drawable/pesan" />

            <ImageView
                android:id="@+id/imageView141"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginEnd="16dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/ic_baseline_arrow_forward_ios_24" />

            <TextView
                android:id="@+id/textView419"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="2dp"
                android:text="TextView"
                app:layout_constraintBottom_toBottomOf="@+id/edtDaftarPegawai2"
                app:layout_constraintStart_toEndOf="@+id/edtDaftarPegawai2"
                app:layout_constraintTop_toTopOf="@+id/edtDaftarPegawai2" />

            <TextView
                android:id="@+id/textView420"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="2dp"
                android:text="17/02/2022"
                app:layout_constraintBottom_toBottomOf="@+id/textView419"
                app:layout_constraintStart_toEndOf="@+id/textView419"
                app:layout_constraintTop_toTopOf="@+id/textView419"
                app:layout_constraintVertical_bias="1.0" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>